.container {
  padding: 0rpx;
}
.list {
  width: 100%;
  box-sizing: border-box;
  padding-bottom: env(safe-area-inset-bottom);
  padding-bottom: constant(safe-area-inset-bottom);
}
.withdraw_item {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  border-bottom: solid 1rpx;
}
.detail_item_head {
  position: relative;
  height: 80rpx;
}
.detail_item_head_code {
  font-weight: 500;
  font-size: 36rpx;
  width: calc( 100% - 150rpx );
}
.detail_item_head_num {
  position: absolute;
  top: 0rpx;
  right: 20rpx;
  font-weight: 500;
  font-size: 36rpx;
}
.detail_item_keys {
  display: inline-block;
  font-size: 30rpx;
  font-weight: 400;
}
.detail_item_keys:not(:first-child) {
  margin-left: 40rpx;
}