/**index.wxss**/
.container {
  padding: 0rpx;
}
.customBar {
  background: #EDEDED;
  font-size: 34rpx;
  font-family: "SFProDisplay-Medium", "SFProDisplay";
  font-weight: 500;
  color: #000000;
  border-bottom: solid 2rpx rgba(0, 0, 0, 0.1);
  transition: all 500ms;
}
.customBarUniverse {
  color: #FFFFFF;
  background: none;
  border-bottom: none;
}
.home_bg {
  width: 100%;
  height: 100%;
}
.home_bg_white.home_bg {
  width: 2000rpx;
  height: 2000rpx;
  position: absolute;
  top: -25%; left: -25%;
  z-index: 10;
  opacity: 1;
  /* transform: scale(2); */
  transition: all 500ms;
}
.home_bg.home_bg_universe {
  opacity: 0.01;
}
.top_box {
  margin: 0rpx 32rpx;
  margin-top: 16rpx;
}
.searchBox {
  width: calc( 100% - 32rpx - 108rpx );
  position: relative;
  margin: auto;
  display: inline-block;
  vertical-align: middle;
}
.searchUniverseBox.searchBox .searchInput {
  background: #fff;
}
.searchUniverseBox.searchBox .searchInputPlaceholder {
  color: #C0C0C0;
}
.searchInput {
  width: 100%;
  border-radius: 40rpx;
  background: #EFEFEF;
  height: 68rpx;
  padding: 0 30rpx;
  padding-left: 80rpx;
  box-sizing: border-box;
  transition: all 300ms;
}
.searchInputPlaceholder {
  color: #272727;
  transition: all 300ms;
}
.searchIcon {
  padding: 0rpx 20rpx;
  padding-left: 28rpx;
  position: absolute;
  top: 50%;
  left: 0rpx;
  transform: translate(0, -40%);
}
.searchIcon>image {
  width: 37rpx;
  height: 37rpx;
}
.content_box {
  position: relative;
  transition: all 500ms;
}
.content_box.hide {
  transform: translate(-100%, 0);
}
.content_box.none {
  height: 0rpx !important;
}
.cut_box {
  width: 108rpx;
  height: 68rpx;
  border-radius: 39rpx;
  display: inline-block;
  margin-left: 32rpx;
  position: relative;
  vertical-align: middle;
}
.cut_bg {
  width: 100%;
  height: 100%;
}
.cut_top_box {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  overflow: hidden;
}
.cut_bg_active {
  width: 108rpx;
  height: 68rpx;
  opacity: 0;
  transition: all 300ms;
}
.cut_bg_active.show {
  opacity: 0.999;
}
.cut_top {
  position: absolute;
  top: 4rpx;
  left: 4rpx;
  width: 60rpx;
  height: 60rpx;
  transition: all 300ms;
}
.cut_top.show {
  left: 44rpx;
  transform: rotate(360deg);
}
.introBox {
  /* position: absolute;
  top: 58rpx;
  left: 0rpx; */
  width: 100%;
  text-align: center;
  margin-top: 58rpx;
}
.introBox .apply_hobby {
  position: absolute;
  top: 60rpx;
  right: 20rpx;
  padding: 20rpx 10rpx;
  border-radius: 20rpx;
  background: rgba(235, 235, 235, 0.5);
  color: #272727;
  font-size: 28rpx;
  font-weight: 600;
  -webkit-writing-mode: vertical-rl;
  writing-mode: vertical-rl;
}
.logo {
  width: 140rpx;
  height: 140rpx;
  display: inline-block;
}
.intro {
  display: block;
  width: 560rpx;
  color: #272727;
  margin: auto;
  line-height: 52rpx;
  font-size: 28rpx;
  font-weight: 400;
  font-family: "PingFangSC-Regular", "PingFang SC";
}
.hobbyBox {
  position: absolute;
  top: 446rpx;
  bottom: 170rpx;
  margin-bottom: env(safe-area-inset-bottom);
  margin-bottom: constant(safe-area-inset-bottom);
  width: 100%;
  text-align: center;
  overflow: hidden;
}
.hobbyBoxLong {
  height: 100%;
  white-space: nowrap;
  position: relative;
  transition: all 500ms;
}
.hobby_dots {
  position: absolute;
  left: 0rpx;
  bottom: 30rpx;
  width: 100%;
  text-align: center;
}
.dot {
  width: 20rpx;
  height: 20rpx;
  border-radius: 20rpx;
  background: #EFEFEF;
  transition: all 500ms;
  margin-left: 20rpx;
  display: inline-block;
}
.dot.active {
  width: 40rpx;
}
.hobby_item {
  display: inline-block;
  vertical-align: top;
  height: 100vw;
  width: 100%;
  position: relative;
}
.hobbyTitle {
  color: #272727;
  font-size: 28rpx;
  font-weight: 600;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  white-space: nowrap;
  transition: all 300ms;
  z-index: 30;
  max-width: 102rpx;
}
.hobby {
  position: absolute;
  transition: all 300ms;
  width: 122rpx;
  height: 122rpx;
  background: rgba(235, 235, 235, 0.5);
  /* border: 10rpx solid #F8F8F8; */
  border-radius: 50%;
}
.hobby::after {
  content: '';
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  width: 102rpx;
  height: 102rpx;
  background: #EBEBEB;
  /* border: 10rpx solid #F8F8F8; */
  border-radius: 50%;
  z-index: 10;
}
.hobby:nth-child(1) {
  left: 104rpx;
  top: 170rpx;
}
.hobby:nth-child(2) {
  left: 250rpx;
  top: 70rpx;
}
.hobby:nth-child(3) {
  left: 490rpx;
  top: 145rpx;
}
.hobby:nth-child(4) {
  left: 357rpx;
  top: 288rpx;
}
.hobby:nth-child(5) {
  left: 85rpx;
  top: 368rpx;
}
.hobby:nth-child(6) {
  left: 443rpx;
  top: 495rpx;
}
.hobby:nth-child(7) {
  left: 578rpx;
  top: 310rpx;
}

.hobbyImg {
  border-radius: 50%;
  overflow: hidden;
}