.universeUtil_top_box,
.universeUtil_right_box,
.universeUtil_bottom_box,
.universeUtil_left_box,
.universeUtil_util_declare_box,
.universeUtil_inshort_box
{
  position: fixed;
  overflow: hidden;
  z-index: 130;
}

.universeUtil_util_declare_box,
.universeUtil_inshort_box {
  /* width: 45vw; */
  top: 10vh;
  /* color: #545454; */
  color: #fff;
  border-radius: 0rpx 50rpx 50rpx 0rpx;
  /* background: #fff; */
  box-sizing: border-box;
  padding: 26rpx 34rpx;
  font-size: 28rpx;
  overflow-y: scroll;
  opacity: 0;
  scrollbar-width: none;
  -ms-overflow-style: none;
}
.universeUtil_inshort_img {
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
}
.universeUtil_inshort_text {
  font-size: 28rpx;
  font-weight: 500;
  /* color: #545454; */
  color: #fff;
  line-height: 56rpx;
  vertical-align: middle;
  padding-left: 4rpx;
}
.universeUtil_universe_box {
  background: #fff;
} 
.universeUtil_universe_box .universeUtil_inshort_text {
  color: #545454;
}
.universeUtil_util_declare_box {
  left: 2vw;
  height: 20rpx;
}
.universeUtil_util_declare_up {
  text-align: center;
}
.universeUtil_util_declare_height {
  animation: height_an 500ms;
  animation-fill-mode: both;
}
.universeUtil_util_declare_fold {
  overflow-y: hidden;
}
.universeUtil_util_declare_fold::after {
  content: "展开操作说明";
  line-height: 40rpx;
  position: absolute;
  top: 0; left: 0;
  width: 100%;
  height: 40rpx;
  text-align: center;
  background: rgba(0, 0, 0);
  opacity: 0.8;
}

@keyframes height_an {
  0% {
    height: 20rpx;
  }
  100% {
    height: 40vh;
  }
}

.universeUtil_inshort_box {
  /* left: 2vw; */
  left: 0rpx;
}
.universeUtil_util_declare_table {
  width: 100%;
}
.universeUtil_util_declare_icon_cell {
  padding-right: 10rpx;
}
.universeUtil_util_declare_icon {
  vertical-align: middle;
}
.universeUtil_util_declare_txt {
  vertical-align: middle;
}

.universeUtil_top_box {
  top: 0rpx;
  left: 15vw;
  width: 70vw;
  height: 150rpx;
  text-align: center;
}
.universeUtil_right_box {
  bottom: 10vh;
  right: 20rpx;
  width: 100rpx;
}
.universeUtil_bottom_box {
  bottom: 0rpx;
  left: 15vw;
  width: 70vw;
  height: 100rpx;
  text-align: center;
}
.universeUtil_left_box {
  bottom: 10vh;
  left: 0rpx;
  width: 100rpx;
}

.universeUtil_top_box .universeUtil_box_item {
  top: 150rpx;
  height: 150rpx;
}
.util_item_title {
  text-align: center;
}

.universeUtil_box_item_bottom {
  position: absolute;
  top: 0; right: 0;
  bottom: 0; left: 0;
  z-index: 10;
  filter: blur(10rpx);
  background: rgba(0, 0, 0, 0.2);
}
.universeUtil_box_item_btn {
  position: absolute;
  top: 0; right: 0;
  bottom: 0; left: 0;
  z-index: 30;
  background: transparent;
  border: none;
  width: 100rpx;
  padding: 0rpx;
}
.universeUtil_box_item_top {
  position: absolute;
  top: 50%; right: 0;
  transform: translate(0, -50%);
  width: 100%;
  z-index: 20;
  text-align: center;
}
.universeUtil_box_item.alone {
  border-radius: 50%;
  margin-bottom: 16rpx;
  height: 100rpx;
}

.universeUtil_box_item {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  /* background: rgba(0, 0, 0, 0.2); */
}
.universeUtil_top_box .universeUtil_box_item:first-child {
  border-bottom-left-radius: 50rpx;
}
.universeUtil_top_box .universeUtil_box_item:last-child {
  border-bottom-right-radius: 50rpx;
}
.universeUtil_right_box .universeUtil_box_item.first {
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
  padding-top: 28rpx;
}
.universeUtil_right_box .universeUtil_box_item:last-child {
  border-bottom-left-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
  padding-bottom: 28rpx;
}
.universeUtil_bottom_box .universeUtil_box_item:first-child {
  border-top-left-radius: 50rpx;
}
.universeUtil_bottom_box .universeUtil_box_item:last-child {
  border-top-right-radius: 50rpx;
}
.universeUtil_left_box .universeUtil_box_item:first-child {
  border-top-right-radius: 50rpx;
}
.universeUtil_left_box .universeUtil_box_item:last-child {
  border-bottom-right-radius: 50rpx;
}
.universeUtil_right_box .universeUtil_box_item
{
  left: 100%;
}
.universeUtil_left_box .universeUtil_box_item
{
  left: -100%;
}
.universeUtil_top_box .universeUtil_box_item
{
  display: inline-block;
  width: 100%;
  top: -100%;
}
.universeUtil_bottom_box .universeUtil_box_item
{
  display: inline-block;
  top: 100%;
}

.util_item_img {
  width: 56rpx;
  height: 56rpx;
  vertical-align: middle;
}
.util_item_text {
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 34rpx;
  text-align: center;
}

.skip_item {
  box-sizing: border-box;
}
.skip_info_box.bg_main {
  border-radius: 20rpx 20rpx 0rpx 0rpx;
}
.skip_info_box {
  padding: 10rpx 20rpx;
  transition: all 300ms;
}
.skip_btns {
  transition: all 300ms;
  height: 0rpx;
  overflow: hidden;
}
.skip_btns.active {
  height: 74rpx;
}
.skip_btn {
  display: inline-block;
  width: 50%;
  height: 74rpx;
  font-size: 20rpx;
  line-height: 74rpx;
  text-align: center;
  color: #fff;
}
.delete {
  background: rgba(255, 51, 0, 0.5);
  border-bottom-right-radius: 20rpx;
}
.setName {
  background: rgb(47, 196, 255, 0.5);
  border-bottom-left-radius: 20rpx;
}
.skip_title {
  font-size: 30rpx;
  font-weight: 400;
  display: inline-block;
  width: calc( 100% - 150rpx );
}
.skip_title_input {
  display: inline-block !important;
  width: 100%;
  vertical-align: middle;
  border-radius: 8rpx;
  background: rgba(0, 0, 0, 0.2);
}
.skip_label {
  font-size: 30rpx;
  display: inline-block;
  width: 150rpx;
  text-align: right;
}
.skip_time {
  font-size: 20rpx;
  text-align: right;
  padding-top: 10rpx;
}
.skip_null {
  text-align: center;
  padding: 20rpx 0rpx;
  font-size: 20rpx;
}

.prop_item {
  width: 60vw;
  box-sizing: border-box;
  padding: 20rpx 0rpx;
  position: relative;
}
.prop_title {
  font-size: 30rpx;
  font-weight: 400;
}
.prop_label {
  font-size: 20rpx;
}
.prop_time {
  position: absolute;
  font-size: 20rpx;
  right: 0rpx;
  top: 50%;
  transform: translate(0, -50%);
}
.prop_null {
  text-align: center;
  padding: 20rpx 0rpx;
  font-size: 20rpx;
}

.popupProp .promptBottomBox {
  filter: blur(10rpx);
  background: rgba(0, 0, 0, 0.2);
}

