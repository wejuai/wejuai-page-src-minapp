.coninfoBox {
  position: fixed;
  width: 100%;
  height: 100vh;
  top: 0; right: 0;
  bottom: 0; left: 0;
  z-index: 100;
}
.maskLayer {
  position: absolute;
  top: 0; right: 0;
  bottom: 0; left: 0;
  width: 100%;
  height: 100vh;
  background: #000;
  opacity: 0.1;
  z-index: 100;
}
.coninfoBody {
  position: absolute;
  width: 200%;
  height: 86vh;
  left: 0; bottom: 0;
  transform: translate(0, 100%);
  z-index: 110;
  overflow-x: hidden;
}
.coninfoStair, .coninfoSub {
  width: 50%;
  height: 100%;
  display: inline-block;
}
.coninfoStair {
  position: relative;
}
.coninfoSub {
  position: absolute;
  width: 50%;
  left: 50%;
  bottom: 0rpx;
  z-index: 120;
  transition: all 300ms ease;
  background: #fff;
}
.translate_x{
  transform: translate(-100%, 0);
}
.parenCom {
  border-bottom: solid 10rpx rgba(246, 246, 246, 1);
}
.scroll scroll-view {
  height: calc( 86vh - 140rpx - env(safe-area-inset-bottom) ) !important;
  height: calc( 86vh - 140rpx - constant(safe-area-inset-bottom) ) !important;
}
.scrollSub croll-view {
  height: calc( 86vh - 140rpx - env(safe-area-inset-bottom) ) !important;
  height: calc( 86vh - 140rpx - constant(safe-area-inset-bottom) ) !important;
}
.coninfoItem {
  transition: all 300ms;
  padding-bottom: 20rpx;
  position: relative;
  overflow: hidden;
}
.com_buttonBox {
  position: absolute;
  top: 0rpx;
  right: -100%;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  box-sizing: border-box;
  padding: 20rpx 10rpx;
  transition: all 300ms;
  background: rgba(255,255,255,0.95);
  text-align: center;
}
.com_buttonBox.show {
  right: 0rpx;
}
.bnbx_item {
  width: 66prx;
  display: inline-block;
  vertical-align: top;
  margin-left: 84rpx;
}
.bnbx_item:first-child {
  margin-left: 0rpx;
}
.bnbx_item image {
  width: 66rpx;
  height: 66rpx;
  margin-bottom: 4rpx;
}
.bnbx_item view {
  font-size: 28rpx;
  font-weight: 400;
  color: #3C3A65;
}
.coninfoTitle {
  padding: 20rpx 0rpx;
  background: #fff;
  position: relative;
  font-weight: 500;
  border-radius: 16rpx 16rpx 0rpx 0rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #272727;
  line-height: 48rpx;
}
.coninfoList {
  position: relative;
  background: #fff;
}
.iconCell {
  text-align: center;
  position: absolute;
  top: 50%;
  right: 34rpx;
  height: 42rpx;
  transform: translate(0, -50%);
}
.iconCell.left {
  right: auto;
  left: 34rpx;
  transform: translate(0, -50%);
}
.commTotal {
  box-sizing: border-box;
  padding: 20rpx 34rpx;
  font-size: 30rpx;
  font-weight: 600;
  color: #272727;
  background: #fff;
}
.iconCell image {
  width: 42rpx;
  height: 42rpx;
}
.title {
  font-size: 30rpx;
  text-align: center;
}
.coverCell {
  width: 130rpx;
  padding-left: 34rpx;
  box-sizing: border-box;
}
.cover {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: auto;
}
.nameCell {
  box-sizing: border-box;
  vertical-align: top;
  position: relative;
  word-break: break-all;
}
.text {
  font-size: 28rpx;
  font-weight: 400;
  color: #545454;
  box-sizing: border-box;
  padding-right: 34rpx;
}
.name {
  font-size: 28rpx;
  font-weight: 600;
  color: #000000;
  line-height: 40rpx;
  position: relative;
}
.moreIcon {
  right: 0rpx;
  padding: 0rpx 34rpx;
  color:  #A3A3A3;
}
.textCell {
  box-sizing: border-box;
}
.subIcon, .starIcon {
  width: 44rpx;
  height: 44rpx;
  vertical-align: middle;
}
.labelCell {
  position: relative;
  text-align: right;
  padding: 10rpx 0rpx;
  box-sizing: border-box;
  color: #A3A3A3;
}
.starBox {
  display: inline-block;
  padding-right: 34rpx;
  text-align: left;
}
.subCommentBox {
  display: inline-block;
  padding-right: 16rpx;
}
.createTime {
  font-size: 24rpx;
  font-weight: 500;
  color: #A3A3A3;
  line-height: 44rpx;
  vertical-align: middle;
  float: left;
}
.author_tag {
  border-radius: 17rpx;
  border: 1rpx solid #A3A3A3;
  padding: 0rpx 12rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #A3A3A3;
  line-height: 32rpx;
  display: inline-block;
}
.subList {
  text-align: left;
  padding-top: 16rpx;
}
.subAll {
  padding: 12rpx 32rpx;
  background: #F6F6F6;
  border-radius: 29rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #545454;
  display: inline-block;
  margin-left: 130rpx;
}
.com_right {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}
.auter_reply {
  font-size: 24rpx;
  font-weight: 500;
  color: #A3A3A3;
  line-height: 44rpx;
  vertical-align: middle;
  float: left;
  padding-left: 8rpx;
}
.createTime text {
  font-size: 24rpx;
  font-weight: 400;
  color: #7383FF;
}
.starNum, .subNum {
  padding-left: 4rpx;
  vertical-align: middle;
  font-size: 24rpx;
  font-weight: 400;
  color: #A3A3A3;
}
.commentsBox {
  position: absolute;
  width: 100%;
  left: 0; bottom: 0;
}