.box {
  display: inline-block;
  position: relative;
  width: 100%;
  height: 100%;
}
.default {
  width: 100%;
  height: 100%;
  max-width: 200rpx;
  max-height: 200rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.default.full {
  width: 100%;
  height: 100%;
  max-width: 100%;
  max-height: 100%;
}
.img {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  left: 0rpx;
  will-change: transform;
  /* z-index: 1; */
}