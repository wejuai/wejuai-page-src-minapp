.dataToast {
  font-size: 20rpx;
  text-align: center;
  padding: 20rpx 0rpx;
  padding-bottom: calc( 30rpx + env(safe-area-inset-bottom) ); /* 兼容 iOS >= 11.2 */
  padding-bottom: calc( 30rpx + constant(safe-area-inset-bottom) ); /* 兼容 iOS < 11.2 */
}
.null_img {
  width: 584rpx;
  height: 538rpx;
}
.notData {
  font-size: 30rpx;
  position: absolute;
  top: 35%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.dataToast .iconfont {
  font-size: 200rpx;
}